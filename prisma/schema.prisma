generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model User {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String   @unique
  password  String
  cods      Json[]
  create_at DateTime @default(now())
}
